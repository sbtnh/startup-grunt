{
    "extends": "airbnb",
    "installedESLint": true,
    "plugins": [
        "react"
    ],
    "env": {
        "browser": true,
        "es6": true,
        "commonjs": true
    },
    "rules":{
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "linebreak-style": ["error", "windows"],
        "no-console": ["error", { "allow": ["warn", "error"] }],
        "no-new": 0,
        "max-len": ["error", { "code": 150 } ],
        "import/prefer-default-export" : 0
    },
    "globals": {
        "$": true,
        "jQuery": true,
        "require": true,
        "google": true
    }
}